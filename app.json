{
  "expo": {
    "name": "MSTracker",
    "slug": "MSApp",
    "version": "1.0.0",
    "scheme": "mstracker",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      "expo-notifications"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.MSApp",
      "googleServicesFile": "./ios/MSTracker/GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "usesNotifications": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.MSApp",
      "googleServicesFile": "./ios/GoogleService-Info.plist",
      "useNextNotificationsApi": true
    },
    "extra": {
      "eas": {
        "projectId": "885611b3-07d6-43e9-b4b2-750cb4d73e07"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/885611b3-07d6-43e9-b4b2-750cb4d73e07"
    }
  }
}
